#include <iostream>
using namespace std;


int main() {
    double PI = 3.14
    double radius;

    cout << "Enter radius: ";
    cin >> radius;

    double area = PI * radius * radius;
    double circumference = 2 * PI * radius;

    cout << "Area of circle is: " << area << "\n";
    cout << "Circumference of circle is: " << circumference << "\n";

    return 0;
