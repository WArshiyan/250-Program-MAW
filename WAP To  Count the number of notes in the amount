#include <iostream>
using namespace std;

int main() {
    int amount;
    cout << "Enter the total amount: ";
    cin >> amount;

    int notes2000 = amount / 2000;
    amount = amount % 2000;

    int notes500 = amount / 500;
    amount = amount % 500;

    int notes200 = amount / 200;
    amount = amount % 200;

    int notes100 = amount / 100;
    amount = amount % 100;

    int notes50 = amount / 50;
    amount = amount % 50;

    int notes20 = amount / 20;
    amount = amount % 20;

    int notes10 = amount / 10;
    amount = amount % 10;

    int notes5 = amount / 5;
    amount = amount % 5;

    int notes1 = amount / 1;

    cout << "Total number of notes:\n";
    cout << "2000: " << notes2000 << endl;
    cout << "500: " << notes500 << endl;
    cout << "200: " << notes200 << endl;
    cout << "100: " << notes100 << endl;
    cout << "50: " << notes50 << endl;
    cout << "20: " << notes20 << endl;
    cout << "10: " << notes10 << endl;
    cout << "5: " << notes5 << endl;
    cout << "1: " << notes1 << endl;

    return 0;
}
